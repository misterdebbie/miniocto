<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>New York Times API</title>
    <link rel="stylesheet" href="style.css">
  </head>
  <body>
    <div id="top">
      <ul>
        <li id="home"><a href="/">Yang</a></li>
        <li><a href="">Runway</a></li>
        <li><a href="">Street</a></li>
        <li><a href="">Mood</a></li>
        <li><a href="">Trends</a></li>
        <li><a href="/view">Saved Articles</a></li>
        <li><a href="http://www.debbiechong.com/">Return to Portfolio</a></li>

        <li id="login"><a href="/account">Log In | Register</a></li>
      </ul>
    </div>
    <div id="header">
      <h1>Journal</h1>
      <p id='timetarget'></p>

      <hr>
      <div id="nav">
        <ul>
          <li><a href="">RTW</a></li>
          <!--<select class="runway" name="runway">
            <option value="fall">Fall 2018</option>
            <option value="spring">Spring 2018</option>
          </select>-->
          <li><a href="">Off Duty</a></li>
          <li><a href="">Trends</a></li>
          <li><a href="">Edits</a></li>
        </ul>
      </div>
      <hr>
    </div>
    <h1>Your Articles</h1>

<% articles.forEach(article => { %>
  <div class="art">
    <h4>
      <a href="<%= `/${article.id}` %>">
        <%= article.title %>
      </a>
    </h4>
    <p><%= article.abstract %></p>
    <p><%= article.byline %></p>
    <p><%= article.thoughts %></p>

  </div>
<% }) %>

  <!--  <div id="footer">
      <p>&copy;yang</p>
    </div>-->
    <script src="date.js" type="text/javascript"></script>
  </body>
</html>
